// Online C++ compiler to run C++ program online
#include <iostream>

using namespace std;

void bubbleSort(int arr[], int num){
    
    for(int i=0; i<num-1; i++){
        
        for(int j=0;j<num-i-1;j++){
            
            if(arr[j]>arr[j+1]){
                
                int temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
                
            }
        }
    }
}

int main() {
    
    int arr[] = {10,19,2,1,7,26};
    int num = sizeof(arr)/sizeof(arr[0]);
    
    bubbleSort(arr,num);
    
    for(int i=0;i<num;i++){
        
        cout<<arr[i];
        cout<<" ";
    }

    return 0;
}
