
#include <iostream>

using namespace std;


void sortArray(int arr[], int num){
    
    for(int i=0; i<num;i++){
        for(int j=0;j<i;j++){
            
            if(arr[i]<arr[j]){
                
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
        
    }
}

int main() {
   
   int arr[] = {23,45,2,6,9,3};
   int num = sizeof(arr)/sizeof(arr[0]);
   
   sortArray(arr,num);
   
   for(int i=0; i<num;i++){
       cout<<arr[i];
       cout<<" ";
   }
   
   return 0;
}
